@model IEnumerable<AdminUsersListingViewModel>

@{
    ViewData["Title"] = "Index";
}

<h2>All Users</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @{
            var count = 1;
            foreach (var user in Model)
            {
        <tr>
            <th scope="row">@(count++)</th>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>@user.Email</td>
            <td><button class="btn-default">Edit</button></td>
        </tr>
            }
        }
    </tbody>
</table>