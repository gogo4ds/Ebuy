@model IEnumerable<AdminUsersListingViewModel>

@{
    ViewData["Title"] = "Index";
}

<h2>All Users</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @{
            var count = 1;
            foreach (var user in Model)
            {
                <tr>
                    <th scope="row">@(count++)</th>
                    <td>Mark</td>
                    <td>Otto</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                </tr>
            }
        }
    </tbody>
</table>